<h1 align='center'>Lista de todas as categorias</h1>

<%= @categories.each do |category| %>
  <ul classe='listing'>
    <li class="row">
      <div class="well col-md-4">
        <li class="article-title">
          <%= link_to "#{category.name}", category_path(category) %>
        </li>
      </div>
    </li>
  </ul>
<% end %>